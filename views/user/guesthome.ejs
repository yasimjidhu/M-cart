<div class="container">
  <div class="row">
    <% data.forEach(function(x) { %>
      <div class="col-sm-12 col-md-4 col-lg-3">
        <div class="iphone" style="position: relative;">
          <!-- Create a link around the image with the product ID as a query parameter -->
          <a href="/productview/<%= x._id %>">
            <img class="product-image" src="/uploads/<%= x.image[0] %>" alt="<%= x.productName %>" />
          </a>
          <i class="fas fa-heart" style="position: absolute; right: 5px; top: 5px;font-size: 35px;color:#001524;"></i>
        </div>
        <div class="details">
          <!-- Display product information dynamically -->
          <h6><%= x.productName %></h6>
          <p>$<%= x.price %></p>
          <a href="/addtocart" class="btn" style="background-color:#001524; color: white; width: 100%;">Add To Cart</a>
        </div>
      </div>
    <% }); %>
  </div>
</div>


